<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <PreferenceCategory app:title="@string/map_display">

        <SwitchPreference
            android:defaultValue="false"
            android:key="Anon"
            android:title="@string/show_anon" />

        <SwitchPreferenceCompat
            android:id="@+id/enableGroup"
            app:key="Group"
            app:title="@string/show_group" />

        <SwitchPreferenceCompat
            android:id="@+id/enableAll"
            app:key="attachment"
            app:title="@string/show_all" />

    </PreferenceCategory>

</PreferenceScreen>